{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "member-access": false,
    "object-literal-sort-keys": false,
    "object-literal-key-quotes": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "max-classes-per-file": [],
    "no-unused-expression": false
  }
}